<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:id="@id/launcher" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:launcher="http://schemas.android.com/apk/res-auto" xmlns:meizu="http://schemas.android.com/apk/res-auto">
    <com.meizu.flyme.launcher.UninstallBlurBg android:id="@id/uninstall_bg" android:background="@color/uninstallbg_color" android:visibility="invisible" android:layout_width="fill_parent" android:layout_height="@dimen/uninstall_bg_height" android:translationY="@dimen/uninstall_bg_init_transy" />
    <com.meizu.flyme.launcher.DragLayer android:id="@id/drag_layer" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <include android:id="@id/mz_title_view" android:visibility="invisible" layout="@layout/title_view" />
        <com.meizu.flyme.launcher.Workspace android:id="@id/workspace" android:layout_width="fill_parent" android:layout_height="fill_parent" meizu:pageSpacing="@dimen/workspace_page_spacing" meizu:pageIndicator="@id/page_indicator" meizu:defaultScreen="@integer/config_workspaceDefaultScreen" />
        <include android:id="@id/hotseat" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/hotseat" />
        <include android:id="@id/overview_panel" android:visibility="gone" layout="@layout/overview_panel" />
        <include android:layout_gravity="center_horizontal" android:id="@id/page_indicator" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" layout="@layout/page_indicator" />
        <com.meizu.flyme.launcher.MzIconPageIndicator android:id="@id/icon_page_indicator" android:layout_width="wrap_content" android:layout_height="@dimen/icon_page_indicator_height" android:layout_marginTop="@dimen/icon_page_indicator_margin_top" android:layout_marginBottom="@dimen/icon_page_indicator_margin_bottom" />
        <include android:id="@id/qsb_bar" layout="@layout/qsb_bar" />
        <com.meizu.flyme.launcher.ScrimView android:id="@id/cling_scrim" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" />
        <include android:id="@id/first_run_cling" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/first_run_cling" />
        <include android:id="@id/workspace_cling" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/workspace_cling" />
        <include android:id="@id/folder_cling" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/folder_cling" />
        <com.meizu.flyme.launcher.DrawableStateProxyView android:layout_gravity="end|center|top" android:id="@id/voice_button_proxy" android:visibility="gone" android:clickable="true" android:layout_width="0.0dip" android:layout_height="0.0dip" android:onClick="onClickVoiceButton" android:importantForAccessibility="no" meizu:sourceViewId="@id/voice_button" />
        <include android:id="@id/apps_customize_pane" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" layout="@layout/apps_customize_pane" />
        <include android:id="@id/arrange_preview_workspace" android:visibility="invisible" layout="@layout/arrange_preview_workspace" />
        <include android:id="@id/widget_group" android:visibility="invisible" layout="@layout/widget_group" />
        <include android:id="@id/mz_wallpaper" android:visibility="invisible" layout="@layout/smart_wallpaper" />
        <include android:id="@id/nfc_share_drop_target" layout="@layout/nfc_share_drop_target" />
    </com.meizu.flyme.launcher.DragLayer>
    <View android:id="@id/mask" android:background="@color/uninstallmask_color" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" android:translationY="@dimen/uninstallmask_init_transy" />
    <include android:id="@id/uninstall_layout" android:visibility="invisible" layout="@layout/uninstall_layout" />
</FrameLayout>